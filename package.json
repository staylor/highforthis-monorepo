{
  "name": "highforthis-monorepo",
  "version": "1.0.0",
  "description": "GraphQL, Web, and Apps",
  "repository": "https://github.com/staylor/highforthis-monorepo",
  "author": "Scott Taylor <scott.c.taylor@mac.com>",
  "license": "private",
  "private": true,
  "engines": {
    "node": ">=18.17.1"
  },
  "sideEffects": false,
  "type": "module",
  "dependencies": {
    "@apollo/server": "4.10.0",
    "@google-cloud/storage": "7.7.0",
    "bcrypt": "5.1.1",
    "body-parser": "1.20.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cron": "3.1.6",
    "dataloader": "2.2.2",
    "env-cmd": "10.1.0",
    "express": "4.18.2",
    "ffprobe": "1.1.2",
    "ffprobe-static": "3.1.0",
    "graphql": "16.8.1",
    "jwt-simple": "0.5.6",
    "lodash.merge": "4.6.2",
    "mkdirp": "3.0.1",
    "mongodb": "6.3.0",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "musicmetadata": "2.0.5",
    "node-fetch": "3.3.2",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "sharp": "0.33.2",
    "slugify": "1.6.6",
    "tsx": "4.7.1"
  },
  "devDependencies": {
    "@remix-run/eslint-config": "2.7.2",
    "@types/bcrypt": "5.0.2",
    "@types/compression": "1.7.5",
    "@types/cors": "2.8.17",
    "@types/eslint": "8.56.3",
    "@types/express": "4.17.21",
    "@types/ffprobe": "1.1.8",
    "@types/ffprobe-static": "2.0.3",
    "@types/lodash.merge": "4.6.9",
    "@types/morgan": "1.9.9",
    "@types/multer": "1.4.11",
    "@types/musicmetadata": "2.0.9",
    "@types/node": "20.11.20",
    "@types/node-fetch": "2.6.11",
    "@types/passport": "1.0.16",
    "@types/passport-jwt": "4.0.1",
    "esbuild": "0.20.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "rimraf": "5.0.5",
    "typescript": "5.3.3"
  },
  "scripts": {
    "graphql:dev": "env-cmd -e dev tsx watch graphql/src/index.ts",
    "graphql:local:dev:start": "env-cmd -e dev node ./graphql/lib/index.js",
    "graphql:local:prod:start": "env-cmd -e prod node ./graphql/lib/index.js",
    "graphql:prod": "./graphql/prod.sh",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --report-unused-disable-directives .",
    "lint-fix": "yarn lint --fix",
    "typecheck:graphql": "tsc -b --config graphql/tsconfig.json graphql",
    "tools:database:dump": "env-cmd -e prod sh ./graphql/tools/database/dump",
    "tools:database:restore": "env-cmd -e prod sh ./graphql/tools/database/restore",
    "tools:artists:apple-music:search:dev": "env-cmd -e dev tsx graphql/tools/apple-music/search/index.ts",
    "tools:artists:apple-music:search:prod": "env-cmd -e prod tsx graphql/tools/apple-music/search/index.ts",
    "tools:venues:geolocation:dev": "env-cmd -e dev tsx graphql/tools/geolocation/index.ts",
    "tools:venues:geolocation:prod": "env-cmd -e prod tsx graphql/tools/geolocation/index.ts"
  }
}
