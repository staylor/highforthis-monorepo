{
  "name": "@highforthis/graphql",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "dependencies": {
    "@apollo/server": "4.11.2",
    "@dotenvx/dotenvx": "1.29.0",
    "@google-cloud/storage": "7.14.0",
    "bcrypt": "5.1.1",
    "body-parser": "1.20.3",
    "compression": "1.7.5",
    "cors": "2.8.5",
    "cron": "3.3.1",
    "dataloader": "2.2.3",
    "express": "4.21.2",
    "ffprobe": "1.1.2",
    "ffprobe-static": "3.1.0",
    "graphql": "16.9.0",
    "jwt-simple": "0.5.6",
    "lodash.merge": "4.6.2",
    "mkdirp": "3.0.1",
    "mongodb": "6.12.0",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "musicmetadata": "2.0.5",
    "node-fetch": "3.3.2",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "sharp": "0.33.5",
    "slugify": "1.6.6"
  },
  "scripts": {
    "dev": "dotenvx run -- tsx watch src/index.ts",
    "prod": "run-s prod:*",
    "prod:stop": "pm2 stop pm2.config.cjs",
    "prod:clean": "rimraf lib",
    "prod:build": "esbuild src/index.ts --packages=external --bundle --platform=node --target=node20.11.1 --format=esm --outfile=lib/index.js",
    "prod:start": "pm2 start pm2.config.cjs",
    "tools:database:data": "yarn tools:database:dump && yarn tools:database:restore",
    "tools:database:dump": "dotenvx run --env-file=.env.production -- sh ./tools/database/dump",
    "tools:database:restore": "sh ./tools/database/restore",
    "tools:script:dev": "dotenvx run -- tsx",
    "tools:script:prod": "dotenvx run --env-file=.env.production -- tsx",
    "typecheck": "tsc"
  }
}
