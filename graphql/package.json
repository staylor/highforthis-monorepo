{
  "name": "@highforthis/graphql",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "dependencies": {
    "@apollo/server": "4.10.0",
    "@google-cloud/storage": "7.7.0",
    "bcrypt": "5.1.1",
    "body-parser": "1.20.2",
    "cors": "2.8.5",
    "cron": "3.1.6",
    "dataloader": "2.2.2",
    "express": "4.18.2",
    "ffprobe": "1.1.2",
    "ffprobe-static": "3.1.0",
    "graphql": "16.8.1",
    "jwt-simple": "0.5.6",
    "mkdirp": "3.0.1",
    "mongodb": "6.3.0",
    "multer": "1.4.5-lts.1",
    "musicmetadata": "2.0.5",
    "node-fetch": "3.3.2",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "sharp": "0.33.3",
    "slugify": "1.6.6"
  },
  "scripts": {
    "dev": "env-cmd -e dev tsx watch src/index.ts",
    "prod": "run-s prod:*",
    "prod:stop": "pm2 stop highforthis-graphql",
    "prod:clean": "rimraf lib",
    "prod:build": "env-cmd -e prod src/index.ts --packages=external --bundle --platform=node --target=node18.17.1 --format=esm --outfile=lib/index.js",
    "prod:start": "env-cmd -e prod pm2 start lib/index.js --name highforthis-graphql",
    "tools:database:dump": "env-cmd -e prod sh ./tools/database/dump",
    "tools:database:restore": "env-cmd -e prod sh ./tools/database/restore",
    "tools:artists:apple-music:search:dev": "env-cmd -e dev tsx tools/apple-music/search/index.ts",
    "tools:artists:apple-music:search:prod": "env-cmd -e prod tsx tools/apple-music/search/index.ts",
    "tools:venues:geolocation:dev": "env-cmd -e dev tsx tools/geolocation/index.ts",
    "tools:venues:geolocation:prod": "env-cmd -e prod tsx tools/geolocation/index.ts"
  }
}