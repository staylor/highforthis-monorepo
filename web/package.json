  "scripts": {
    "dev": "dotenvx run -- node server.mjs",
    "prod": "run-s prod:*",
    "prod:clean": "rimraf build node_modules/.vite",
    "prod:build": "NODE_ENV=production react-router build",
    "prod:upload": "gsutil -m -h 'Cache-control:public, max-age=259200' rsync -r build/client gs://static.highforthis.com/build",
    "prod:stop": "pm2 stop pm2.config.cjs",
    "prod:start": "pm2 start pm2.config.cjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:update": "vitest -u run",
    "typecheck": "react-router typegen && tsc"
  }
