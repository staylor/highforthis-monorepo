    "@lexical/react": "0.14.3",
    "@remix-run/express": "2.8.1",
    "@remix-run/node": "2.8.1",
    "@remix-run/react": "2.8.1",
    "@remix-run/server-runtime": "2.8.1",
    "classnames": "2.5.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "express": "4.19.2",
    "filesize": "10.1.1",
    "graphql": "16.8.1",
    "html-escape": "2.0.0",
    "http-proxy-middleware": "3.0.0",
    "isbot": "5.1.4",
    "lexical": "0.14.3",
    "lodash.debounce": "4.0.8",
    "morgan": "1.10.0",
    "qs": "6.12.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-auth": "3.6.0",
    "remix-auth-form": "1.4.0",
    "tiny-invariant": "1.3.3"
  },
  "devDependencies": {
    "@remix-run/dev": "2.8.1",
    "@tailwindcss/forms": "0.5.7",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.2",
    "@testing-library/user-event": "14.5.2",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.19",
    "eslint-plugin-tailwindcss": "3.15.1",
    "happy-dom": "14.7.0",
    "postcss": "8.4.38",
    "prettier-plugin-tailwindcss": "0.5.13",
    "tailwindcss": "3.4.3",
    "vite": "5.2.8",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.4.0"
  },
  "scripts": {
    "dev": "dotenvx run -- node --no-warnings=WARN_JSON_IMPORT_EXPERIMENTAL server.mjs",
    "prod": "run-s prod:*",
    "prod:clean": "rimraf build node_modules/.vite",
    "prod:build": "NODE_ENV=production remix vite:build",
    "prod:upload": "gsutil -m rsync -r build/client gs://static.highforthis.com/build",
    "prod:metadata": "gsutil -m setmeta -r -h 'Cache-control:public, max-age=259200' gs://static.highforthis.com/build",
    "prod:stop": "pm2 stop pm2.config.cjs",
    "prod:start": "pm2 start pm2.config.cjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:update": "vitest -u run"
  }
}