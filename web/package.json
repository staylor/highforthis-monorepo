{
  "name": "@highforthis/web",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "dependencies": {
    "@apollo/client": "3.9.5",
    "@lexical/react": "0.13.1",
    "@remix-run/express": "2.8.0",
    "@remix-run/node": "2.8.0",
    "@remix-run/react": "2.8.0",
    "@remix-run/server-runtime": "2.8.0",
    "classnames": "2.5.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "env-cmd": "10.1.0",
    "express": "4.18.2",
    "filesize": "10.1.0",
    "graphql": "16.8.1",
    "html-escape": "2.0.0",
    "http-proxy-middleware": "2.0.6",
    "isbot": "5.1.0",
    "lexical": "0.13.1",
    "lodash.debounce": "4.0.8",
    "morgan": "1.10.0",
    "qs": "6.11.2",
    "query-types": "0.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-auth": "3.6.0",
    "remix-auth-form": "1.4.0",
    "tiny-invariant": "1.3.3"
  },
  "devDependencies": {
    "@remix-run/dev": "2.8.0"
  },
  "scripts": {
    "dev": "env-cmd -e dev node --no-warnings=WARN_JSON_IMPORT_EXPERIMENTAL server.mjs",
    "prod": "run-s prod:*",
    "prod:clean": "rimraf build node_modules/.vite",
    "prod:build": "env-cmd -e prod remix vite:build",
    "prod:upload": "gsutil -m rsync -r public gs://static.highforthis.com",
    "prod:metadata": "gsutil -m setmeta -r -h 'Cache-control:public, max-age=259200' gs://static.highforthis.com/build",
    "prod:stop": "pm2 stop highforthis-remix",
    "prod:start": "pm2 start pm2.config.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:update": "vitest -u run"
  }
}